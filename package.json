{
    "name": "shuffle-bot",
    "version": "1.1.0",
    "description": "A networking bot for Discord that shuffles and distributes people into voice corners.",
    "author": "Benedikt Magnus",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenediktMagnus/ShuffleBot.git"
    },
    "keywords": [
        "bot",
        "discord"
    ],
    "engines": {
        "node": ">=16.0.0"
    },
    "dependencies": {
        "@discordjs/builders": "0.6.0",
        "@discordjs/rest": "0.1.0-canary.0",
        "discord-api-types": "0.23.1",
        "discord.js": "13.1.0"
    },
    "devDependencies": {
        "@types/node": "16.11.59",
        "@types/node-fetch": "2.6.2",
        "@typescript-eslint/eslint-plugin": "5.38.0",
        "@typescript-eslint/parser": "5.38.0",
        "eslint": "8.23.1",
        "typescript": "4.8.3"
    },
    "files": [
        "bin",
        "data/*.default",
        "LICENSE",
        "README.md"
    ],
    "main": "bin/main.js",
    "scripts": {
        "build": "npx tsc",
        "clean": "rm -rf bin",
        "lint": "npx eslint \"src/**/*.ts\"",
        "prepack": "npm run clean && npx tsc -p tsconfig.release.json",
        "start": "node bin/main.js"
    },
    "private": true
}
