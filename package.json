{
    "name": "shuffle-bot",
    "description": "A networking bot for Discord that shuffles and distributes people into voice corners.",
    "author": "Benedikt Magnus",
    "version": "0.1.0-dev",
    "keywords": [
        "bot",
        "discord"
    ],
    "engines": {
        "node": ">=14.15.0"
    },
    "dependencies": {
        "discord.js": "13.1.0",
        "@discordjs/builders": "0.5.0",
        "@discordjs/rest": "0.1.0-canary.0"
    },
    "devDependencies": {
        "@types/node": "14.17.9",
        "@typescript-eslint/eslint-plugin": "4.29.1",
        "@typescript-eslint/parser": "4.29.1",
        "eslint": "7.32.0",
        "typescript": "4.3.5"
    },
    "files": [
        "bin",
        "README.md"
    ],
    "main": "bin/main.js",
    "scripts": {
        "build": "npx tsc",
        "clean": "rm -rf bin && rm -rf coverage",
        "lint": "npx eslint \"src/**/*.ts\"",
        "prepack": "npm run clean && npx tsc -p tsconfig.release.json",
        "start": "node bin/main.js",
    },
    "private": true
}
